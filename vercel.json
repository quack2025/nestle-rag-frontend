{
  "name": "nestle-rag-frontend",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_APP_ENV": "production"
  },
  "build": {
    "env": {
      "VITE_API_URL": "https://web-production-68331.up.railway.app",
      "VITE_JWT_SECRET": "tu-jwt-secret-key-nestle-2024",
      "VITE_APP_ENV": "production",
      "VITE_DEBUG": "false"
    }
  }
}